<html lang="en">

<head>
    <%- include('../components/head'); %>
</head>

<body class="container">

    <header>
        <%- include('../components/header'); %>
    </header>

    <main>
        <h1>Contracts</h1>
        <div id='contractsRoot' class="maps">
            <% for (var i=0; i < contracts.length; i++) { %>
                <div class='card contracts__contract' >
                    <a href=/statements?contractId=<%= contracts[i].name %>><%= contracts[i].name %></a>
                       
                    
                </div>
            <% } %>
        </div>
    </main>

    <footer>
        <%- include('../components/footer'); %>
    </footer>
    <script src='js/contracts.js'></script>
</body>

</html>